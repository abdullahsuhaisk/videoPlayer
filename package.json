  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/video.js": "^7.2.10",
    "eslint-config-airbnb": "^17.1.0",
