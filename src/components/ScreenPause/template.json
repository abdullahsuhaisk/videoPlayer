{
  "widgets": [
    {
      "type": "Empty",
      "props": {
        "children": [
          {
            "type": "Login",
            "props": {
              "styles": {
                ".vb--login-close": {
                  "position": "absolute",
                  "top": "0",
                  "left": "0",
                  "padding": "10px 15px",
                  "fontSize": "24px",
                  "color": "#0008",
                  "cursor": "pointer",
                  "outline": "none",
                  " .vb--login-close:hover": {
                    "color": "#00acd8",
                    "backgroundColor": "#FFF",
                    "border": "1px solid #00acd8"
                  },
                  ".vb--login-close::after": {
                    "content": "aaa",
                    "color": "#00ffd2",
                    "backgroundColor": "#FFF",
                    "border": "10px solid #00acd8"
                  }
                }
              }
            }
          },
          {
            "type": "Empty",
            "props": {
              "styles": {
